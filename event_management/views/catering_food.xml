<?xml version="1.0" encoding="utf-8"?>
<odoo>
            <!-- Used for showing tree views under each pages in event catering-->
   <record id="catering_food_view_list" model="ir.ui.view">
        <field name="name">catering.food.list</field>
        <field name="model">catering.food</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="item_id"
                       options="{'no_create': True, 'no_create_edit':True}"/>
                <field name="description"/>
                <field name="quantity"/>
                <field name="uom"/>
                <field name="unit_price"/>
                <field name="subtotal"/>
            </tree>
        </field>
    </record>
</odoo>